<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css">
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.css">
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>

<div class="container justify-content-center my-5">
    <div class="row justify-content-center my-5">

        <table class="table table-dark table-responsive-sm">
            <thead>
            <tr>
                <th scope="col">Ime predmeta</th>
                <th scope="col">Datum uzimanja</th>
                <th scope="col">Ime iznajmljivaca</th>


            </tr>
            </thead>
            <% data.forEach(function(x) { %>
            <tbody>

            <tr>
                <form action="/vratipredmet/<%=x.id%>" method="post">
                    <td><input type="text" name="ime_predmeta" readonly="readonly" id="edit" value="<%=x.ime_predmeta%>"></td>
                    <td><input type="text" name="datum_uzimanja"  readonly="readonly" id="edit" value="<%=x.datum_uzimanja%>"></td>
                    <td><input type="text" name="ime_korisnika"   readonly="readonly" id="edit" value="<%=x.ime%>"></td>
                    <input type="hidden" name="id_korisnika" readonly="readonly" id="edit" value="<%=x.idkor%>">

            </tr>

            </tbody>

            <% }) %>
        </table>
        <input type="submit" value="Vrati ovaj predmet">
        </form>

    </div>
</div>

<script type="text/javascript" src="/jquery.js"></script>
<script type="text/javascript" src="/js/bootstrap.js"></script>
</body>
</html>
